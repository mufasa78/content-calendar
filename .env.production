# Production Environment Variables Template
# These will be set in Render's dashboard - DO NOT put real values here

# Database (automatically provided by Render)
DATABASE_URL=postgresql://user:password@host:port/database

# Clerk Authentication (set these in Render dashboard)
CLERK_PUBLISHABLE_KEY=pk_live_xxxxx
CLERK_SECRET_KEY=sk_live_xxxxx
VITE_CLERK_PUBLISHABLE_KEY=pk_live_xxxxx

# Google Calendar API (set these in Render dashboard)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=https://your-app.onrender.com/api/google/callback

# Server Configuration
PORT=10000
NODE_ENV=production

# Optional API Key
API_KEY=your_api_key